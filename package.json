{
  "name": "graphite-helpers",
  "displayName": "graphite-helpers",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.99.3"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/extension.js",
  "activationEvents": [
    "onCommand:graphite-helpers.graphiteModify",
    "onCommand:graphite-helpers.graphiteSubmitStack"
  ],
  "contributes": {
    "commands": [
      {
        "command": "graphite-helpers.graphiteModify",
        "title": "Graphite: Modify"
      },
      {
        "command": "graphite-helpers.graphiteSubmitStack",
        "title": "Graphite: Submit Stack"
      },
      {
        "command": "graphite-helpers.graphiteUp",
        "title": "Graphite: Up"
      },
      {
        "command": "graphite-helpers.graphiteDown",
        "title": "Graphite: Down"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "pnpm run package",
    "compile": "pnpm run check-types && pnpm run lint && node esbuild.cjs",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.cjs --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "pnpm run check-types && pnpm run lint && node esbuild.cjs --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "pnpm run compile-tests && pnpm run compile && pnpm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.99.3",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "esbuild": "^0.25.8",
    "eslint": "^9.32.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "execa": "^9.6.0"
  }
}
